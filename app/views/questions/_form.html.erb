<%= form_with model: @question, local: true do |f|%>
  <div class="field">
    <%= f.label :title, "問題のタイトル" %><br />
    <%= f.text_field :title, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :overview, "問題の概要" %><br />
    <%= f.text_area :overview, class: :form__text, id:"prototype_catch_copy" %>
  </div>


  <div class="field">
    <%= f.label :image, "問題の画像" %><br />
    <%= f.file_field :image, id:"prototype_image" %>
  </div>

  <div class="actions">
    <%= f.submit "投稿する", class: :form__btn  %>
  </div>
<% end %>